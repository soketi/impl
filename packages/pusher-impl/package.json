{
  "name": "@soketi/pusher-impl",
  "version": "1.0.0",
  "description": "TypeScript boilerplate to use on your Pusher implementations. Made with ❤️ at Soketi.",
  "author": "soketi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soketi/impl"
  },
  "keywords": [
    "soketi",
    "websocket",
    "typescript",
    "boilerplate",
    "uWebSockets.js",
    "pusher",
    "pusher-js"
  ],
  "scripts": {
    "aegir": "aegir",
    "clean": "aegir clean",
    "lint": "aegir lint --bail false",
    "dep-check": "aegir dep-check",
    "build": "aegir build"
  },
  "devDependencies": {
    "@types/pusher-js": "^5.1.0",
    "aegir": "^40.0.8"
  },
  "dependencies": {
    "@soketi/impl": "^1.0.0",
    "pusher": "^5.1.3",
    "pusher-js": "^8.3.0"
  },
  "type": "module",
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./apps": {
      "import": "./dist/src/apps/index.js",
      "types": "./dist/src/apps/index.d.ts"
    },
    "./channels": {
      "import": "./dist/src/channels/index.js",
      "types": "./dist/src/channels/index.d.ts"
    },
    "./channels/presence": {
      "import": "./dist/src/channels/presence/index.js",
      "types": "./dist/src/channels/presence/index.d.ts"
    },
    "./gossiper": {
      "import": "./dist/src/gossiper/index.js",
      "types": "./dist/src/gossiper/index.d.ts"
    },
    "./ws": {
      "import": "./dist/src/ws/index.js",
      "types": "./dist/src/ws/index.d.ts"
    },
    "./pusher-utils": {
      "import": "./dist/src/pusher-utils.js",
      "types": "./dist/src/pusher-utils.d.ts"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
