{
  "name": "@soketi/impl",
  "version": "1.0.0",
  "description": "Common TypeScript boilerplate used to build a basic WebSocket Server implementation, which can be extended to implement your own. Made with ❤️ at Soketi.",
  "author": "soketi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soketi/impl"
  },
  "keywords": [
    "soketi",
    "websocket",
    "typescript",
    "boilerplate",
    "uWebSockets.js",
    "pusher",
    "pusher-js"
  ],
  "scripts": {
    "aegir": "aegir",
    "clean": "aegir clean",
    "lint": "aegir lint --bail false",
    "dep-check": "aegir dep-check",
    "build": "aegir build"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.22.20",
    "@libp2p/interface": "^0.1.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.6.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "aegir": "^40.0.8",
    "consul": "^1.2.0",
    "eslint": "^8.40.0",
    "typescript": "^5.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.24.0",
    "vitest": "^0.31.0",
    "ws": "^8.13.0"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "^10.0.0",
    "@chainsafe/libp2p-noise": "^13.0.1",
    "@libp2p/bootstrap": "^9.0.4",
    "@libp2p/kad-dht": "^10.0.7",
    "@libp2p/mdns": "^9.0.4",
    "@libp2p/mplex": "^9.0.5",
    "@libp2p/tcp": "^8.0.5",
    "@soketi/impl-interfaces": "^1.0.0",
    "aegir": "^40.0.13",
    "arraybuffer-to-string": "^1.0.2",
    "dot-wild": "^3.0.1",
    "ioredis": "^5.3.2",
    "itty-router": "^4.0.9",
    "libp2p": "^0.46.6"
  },
  "type": "module",
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./brain": {
      "import": "./dist/src/brain/index.js",
      "types": "./dist/src/brain/index.d.ts"
    },
    "./gossiper": {
      "import": "./dist/src/gossiper/index.js",
      "types": "./dist/src/gossiper/index.d.ts"
    },
    "./prospector": {
      "import": "./dist/src/prospector/index.js",
      "types": "./dist/src/prospector/index.d.ts"
    },
    "./ws": {
      "import": "./dist/src/ws/index.js",
      "types": "./dist/src/ws/index.d.ts"
    },
    "./utils": {
      "import": "./dist/src/utils.js",
      "types": "./dist/src/utils.d.ts"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
